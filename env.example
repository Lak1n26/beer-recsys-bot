REDIS_URL=redis://localhost:6379/0

# Session storage
SESSION_STORAGE_PATH=/app/storage/sessions
SESSION_SECRET=replace-me-with-strong-secret

# External integrations
SOY_TOKEN=put-your-yandex-oauth-token-here
