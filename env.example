# Core application settings
TELEGRAM_TOKEN=replace-with-telegram-token
LOG_LEVEL=INFO

# Database credentials
POSTGRES_DB=postgres17
POSTGRES_USER=postgres17
POSTGRES_PASSWORD=postgres17

# Connection URLs (override if you deploy externally)
